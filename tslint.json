{
  "extends": "tslint:latest",
  "rules": {
    "quotemark": false,
    "max-line-length": [
      true,
      200
    ],
    "object-literal-shorthand": false,
    "typedef": [
      true,
      "call-signature",
      "property-declaration"
    ],
    "no-implicit-dependencies": [true, "dev"],
    "no-shadowed-variable": false,
    "prefer-object-spread": false
  }
}
